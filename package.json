{
  "name": "modern-browsers",
  "version": "1.4.0",
  "description": "Regexp of modern browsers",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com:christophehurpeau/modern-browsers.git",
  "homepage": "https://github.com/christophehurpeau/modern-browsers",
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node10-dev.mjs",
          "require": "./dist/index-node10-dev.cjs.js"
        },
        "import": "./dist/index-node10.mjs",
        "require": "./dist/index-node10.cjs.js"
      }
    }
  },
  "module:node": "./dist/index-node10.mjs",
  "module:node-dev": "./dist/index-node10-dev.mjs",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf dist",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "tsc && eslint --ext .js,.ts --quiet *.js src",
    "postbuild": "tsc -p tsconfig.build.json",
    "preversion": "yarn run lint && yarn run build && repository-check-dirty",
    "release": "repository-check-dirty && standard-version -a -m 'chore(release): %s [skip ci]' && git push --follow-tags origin master && npm publish",
    "test": "jest",
    "test:watch": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@pob/commitlint-config": "1.1.2",
    "@pob/eslint-config": "40.0.0",
    "@pob/eslint-config-node": "40.0.0",
    "@pob/eslint-config-typescript": "40.0.0",
    "@pob/root": "2.3.1",
    "@types/jest": "26.0.14",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "babel-jest": "26.5.2",
    "babel-preset-latest-node": "5.0.0",
    "babel-preset-modern-browsers": "15.0.1",
    "eslint": "7.11.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-unicorn": "22.0.0",
    "husky": "4.3.0",
    "jest": "26.5.2",
    "pob-babel": "25.2.0",
    "pob-lcov-reporter": "3.0.0",
    "prettier": "2.1.2",
    "rollup": "2.29.0",
    "standard-version": "9.0.0",
    "typescript": "4.0.3",
    "xunit-file": "^1.0.0"
  }
}
